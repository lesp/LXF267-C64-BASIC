10 PRINT "ENTER FIVE RESPONSES"
20 FOR I=1 TO 5
30 PRINT "RESPONSE";I;:INPUT S$(I)
40 NEXT I
50 PRINT "THINK OF A QUESTION AND PRESS Y AND ENTER TO START"
60 INPUT "PRESS Y TO START OR X TO EXIT"; A$
70 IF A$="Y" THEN GOTO 100
80 PRINT "THANKS FOR PLAYING"
90 GOTO 130
100 X=INT(RND(1)*5)
110 PRINT S$(X)
120 GOTO 50
130 END
